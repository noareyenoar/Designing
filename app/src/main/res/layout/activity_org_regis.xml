<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".org_regis">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/hosregis_textview_service4"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="34dp"
            android:gravity="center"
            android:text="Special list"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/hosregis_spinner_speciallist"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_textview_service2" />

        <TextView
            android:id="@+id/hosregis_textview_picture"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="85dp"
            android:gravity="center"
            android:text="Picture"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/hosregis_imageview_addpicture"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_textview_service4" />

        <TextView
            android:id="@+id/hosregis_textview_service2"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="59dp"
            android:gravity="center"
            android:text="Species"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/hosregis_spinner_species"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/opd_textview_weight4" />

        <EditText
            android:id="@+id/opd_edittext_weight4"
            android:layout_width="271dp"
            android:layout_height="88dp"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:inputType="textMultiLine"
            android:lines="3"
            android:maxLines="10"
            android:minLines="1"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@+id/opd_textview_weight4"
            app:layout_constraintTop_toBottomOf="@+id/hos_service_spinner" />

        <TextView
            android:id="@+id/opd_textview_weight4"
            android:layout_width="91dp"
            android:layout_height="33dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="52dp"
            android:gravity="center"
            android:text="Other service"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/opd_edittext_weight4"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hos_service_spinner" />

        <TextView
            android:id="@+id/hosregis_textview_service"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:gravity="center"
            android:text="Service"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/hos_service_spinner"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_button_location" />

        <EditText
            android:id="@+id/opd_edittext_weight2"
            android:layout_width="122dp"
            android:layout_height="36dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="104dp"
            android:ems="10"
            android:inputType="number"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/hosregis_textview_tel"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_imageview_logo" />

        <TextView
            android:id="@+id/hosregis_textview_tel"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="36dp"
            android:gravity="center"
            android:text="Tel"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toStartOf="@+id/opd_edittext_weight2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_imageview_logo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="87dp"
            android:layout_height="45dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:gravity="center"
            android:text="Enterprise"
            android:textColor="#000000"
            android:textSize="18dp"
            app:layout_constraintEnd_toStartOf="@+id/editText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="207dp"
            android:layout_height="45dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="8dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toStartOf="@+id/register_opd"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" />

        <Spinner
            android:id="@+id/hos_service_spinner"
            android:layout_width="276dp"
            android:layout_height="39dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/hosregis_textview_service"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_button_location" />

        <ImageView
            android:id="@+id/hosregis_imageview_logo"
            android:layout_width="194dp"
            android:layout_height="193dp"
            android:layout_marginStart="105dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="105dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:srcCompat="@drawable/add_logo" />

        <Button
            android:id="@+id/hosregis_button_location"
            android:layout_width="192dp"
            android:layout_height="40dp"
            android:layout_marginStart="108dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="108dp"
            android:text="Location"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_button_bussinesshour" />

        <Button
            android:id="@+id/hosregis_button_bussinesshour"
            android:layout_width="194dp"
            android:layout_height="37dp"
            android:layout_marginStart="108dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="108dp"
            android:text="Bussines Hour"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/opd_edittext_weight2" />

        <Spinner
            android:id="@+id/hosregis_spinner_species"
            android:layout_width="280dp"
            android:layout_height="39dp"
            android:layout_marginTop="30dp"
            app:layout_constraintStart_toEndOf="@+id/hosregis_textview_service2"
            app:layout_constraintTop_toBottomOf="@+id/opd_edittext_weight4" />

        <Spinner
            android:id="@+id/hosregis_spinner_speciallist"
            android:layout_width="280dp"
            android:layout_height="39dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="29dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/hosregis_textview_picture"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_spinner_species" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1000dp"
            android:text="Button"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="203dp" />

        <ImageView
            android:id="@+id/hosregis_imageview_addpicture"
            android:layout_width="127dp"
            android:layout_height="98dp"
            android:layout_marginTop="45dp"
            app:layout_constraintStart_toEndOf="@+id/hosregis_textview_picture"
            app:layout_constraintTop_toBottomOf="@+id/hosregis_spinner_speciallist"
            app:srcCompat="@drawable/add" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>