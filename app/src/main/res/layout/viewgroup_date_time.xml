<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/date_time_viewgroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#E407ECBE">


        <TextView
            android:id="@+id/textView17"
            android:layout_width="35dp"
            android:layout_height="13dp"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="73dp"
            android:text="O'Clock"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vg_spinner_dayto" />

        <Spinner
            android:id="@+id/vg_spinner_dayfrom"
            android:layout_width="172dp"
            android:layout_height="32dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="14dp"
            app:layout_constraintEnd_toStartOf="@+id/vg_spinner_dayto"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Spinner
            android:id="@+id/vg_spinner_dayto"
            android:layout_width="166dp"
            android:layout_height="32dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vg_spinner_dayfrom"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginEnd="29dp"
            android:layout_marginBottom="6dp"
            android:text="-"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/vg_textview_timeto"
            app:layout_constraintStart_toEndOf="@+id/textView11" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="35dp"
            android:layout_height="11dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="41dp"
            android:layout_marginBottom="6dp"
            android:text="O'Clock"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vg_textview_timefrom"
            app:layout_constraintTop_toBottomOf="@+id/vg_spinner_dayfrom" />

        <TextView
            android:id="@+id/vg_textview_timefrom"
            android:layout_width="70dp"
            android:layout_height="27dp"
            android:layout_marginStart="78dp"
            android:layout_marginTop="31dp"
            android:gravity="center"
            android:text="Time"
            android:textColor="#0047D4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vg_spinner_dayfrom" />

        <TextView
            android:id="@+id/vg_textview_timeto"
            android:layout_width="65dp"
            android:layout_height="27dp"
            android:layout_marginTop="31dp"
            android:layout_marginEnd="1dp"
            android:gravity="center"
            android:text="Time"
            android:textColor="#0047D4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/vg_spinner_dayto" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
